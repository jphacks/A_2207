{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "incremental": true,
    "types": [
      "@emotion/react/types/css-prop"
    ],
    "jsxImportSource": "@emotion/react"
  },
  "exclude": [
    "node_modules",
    "deployments",
    ".next",
    "out"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/vrm/loadMixamoAnimation.js",
    "src/components/vrm/mixamoToVrm.js"
  ]
}